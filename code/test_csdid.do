local cnt=0
foreach i in 2004 2006 2007 {		
	foreach j in 2004 2005 2006 2007 {
	    
		local time1 = min(`i'-1, `j'-1)
		local eqnm `eqnm' g`i'
		local ccnm `ccnm' t_`time1'_`j'
		
	    if `cnt'==0 {
		    local vlabrif  ( wgt  _g`i'_`time1'_`j' = )
		}
		else	local vlabrif `vlabrif'  (_g`i'_`time1'_`j' = ) 
		local cnt = `cnt'+1
	}
}
display "`vlabrif'"

foreach i in 2 3 4 {		
	local vlabrif `vlabrif' (f_`i'= ) 
}

foreach i in 2004 2006 2007 {		
    local eqnm `eqnm' wgt
	local ccnm `ccnm' w`i'
}
display "`vlabrif'"

ml model lf csdid_ols `vlabrif', maximize robust miss
matrix b=e(b)
matrix V=e(V)

matrix coleq b =`eqnm'
matrix coleq V =`eqnm'
matrix roweq V =`eqnm'

matrix colname b =`ccnm'
matrix colname V =`ccnm'
matrix rowname V =`ccnm'

adde repost b=b V=V, rename
ereturn display

